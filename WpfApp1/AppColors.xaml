<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--大背景-->
    <LinearGradientBrush x:Key="all-Background" EndPoint="0.8,0.8" StartPoint="0,0" Opacity="1" MappingMode="RelativeToBoundingBox">
        <GradientStop Offset="0"/>
        <GradientStop Offset="0.993"/>
    </LinearGradientBrush>
    <!--按钮/tab标签选中-->
    <SolidColorBrush x:Key="button"/>
    <!--svg图标按钮-->
    <SolidColorBrush x:Key="svg-icon" Color="#FF4B728D"/>
    <!--隧道底色-->
    <SolidColorBrush x:Key="list-Background" Color="Black"/>
    <!--tab标签底色-->
    <SolidColorBrush x:Key="tabF-Background"/>
    <!--边框选中-->
    <SolidColorBrush x:Key="mo-BorderBrush" Color="#FF797979"/>
</ResourceDictionary>